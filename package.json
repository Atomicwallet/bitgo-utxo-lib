{
  "name": "bitgo-utxo-lib",
  "version": "1.3.1",
  "description": "Client-side Bitcoin JavaScript library",
  "main": "./src/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "bitgo",
    "utxo",
    "bitcoin",
    "zcash",
    "dash",
    "litecoin"
  ],
  "scripts": {
    "coverage-report": "nyc report --reporter=lcov",
    "coverage-html": "nyc report --reporter=html",
    "coverage": "nyc --check-coverage --branches 90 --functions 90 mocha",
    "integration": "mocha --require ts-node/register \"test/integration/*.ts\"",
    "unit": "mocha --require ts-node/register \"test/*.ts\"",
    "build": "tsc --project tsconfig.build.json",
    "postinstall": "npm run build",
    "test": "mocha --require ts-node/register \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BitGo/bitgo-utxo-lib.git"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@types/node": "^11.13.4",
    "bech32": "0.0.3",
    "bigi": "^1.4.0",
    "bip66": "^1.1.0",
    "bitcoin-ops": "^1.3.0",
    "blake2b": "https://github.com/BitGo/blake2b#6268e6dd678661e0acc4359e9171b97eb1ebf8ac",
    "bs58check": "^2.0.0",
    "create-hash": "^1.1.0",
    "create-hmac": "^1.1.3",
    "debug": "~3.1.0",
    "ecurve": "^1.0.0",
    "eslint": "^5.16.0",
    "merkle-lib": "^2.0.10",
    "pushdata-bitcoin": "^1.0.1",
    "randombytes": "^2.0.1",
    "safe-buffer": "^5.0.1",
    "typeforce": "^1.11.3",
    "typescript": "^3.4.3",
    "varuint-bitcoin": "^1.0.4",
    "wif": "^2.0.1"
  },
  "devDependencies": {
    "@types/mocha": "~5.2.6",
    "bip39": "^2.3.0",
    "bip65": "^1.0.1",
    "bs58": "^4.0.0",
    "dhttp": "^2.4.2",
    "minimaldata": "^1.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "proxyquire": "^1.4.0",
    "sinon": "^1.12.2",
    "ts-node": "^8.0.3",
    "tslint": "~5.14.0",
    "tslint-eslint-rules": "~5.4.0",
    "eslint": "~5.15.3"
  },
  "license": "MIT",
  "optionalDependencies": {
    "secp256k1": "^3.5.2"
  }
}
